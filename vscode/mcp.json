{
	"servers": {
		"github/github-mcp-server": {
			"type": "http",
			"url": "https://api.githubcopilot.com/mcp/",
			"gallery": "https://api.mcp.github.com/2025-09-15/v0/servers/ab12cd34-5678-90ef-1234-567890abcdef",
			"version": "0.13.0"
		},
		"atlassian/atlassian-mcp-server": {
			"type": "http",
			"url": "https://mcp.atlassian.com/v1/sse",
			"gallery": "https://api.mcp.github.com/2025-09-15/v0/servers/28c650c6-5e61-4ab7-9eb2-505be6350476",
			"version": "1.0.0"
		},
		"browsermcp": {
			"type": "stdio",
			"command": "npx",
			"args": [
				"@browsermcp/mcp@latest"
			]
		},
		"microsoft/playwright-mcp": {
			"type": "stdio",
			"command": "npx",
			"args": [
				"@playwright/mcp@latest"
			],
			"gallery": "https://api.mcp.github.com/2025-09-15/v0/servers/41b79849-7e6c-4fc7-82c0-5a611ea21523",
			"version": "0.0.1-seed"
		}
	},
	"inputs": [
		{
			"id": "tfe_token",
			"type": "promptString",
			"description": "Terraform API Token",
			"password": true
		},
		{
			"id": "tfe_hostname",
			"type": "promptString",
			"description": "Terraform hostname",
			"password": false
		}
	]
}